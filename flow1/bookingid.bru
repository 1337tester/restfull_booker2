meta {
  name: bookingid
  type: http
  seq: 2
}

get {
  url: {{url}}/booking?firstname={{firstname}}&lastname={{lastname}}
  body: none
  auth: none
}

params:query {
  firstname: {{firstname}}
  lastname: {{lastname}}
}

assert {
  res.status: eq 200
  res.body[0].bookingid: isNumber 
}

script:post-response {
  console.log("First bookingid is " + res.body[0].bookingid);
  
  bru.setEnvVar("bookingid", res.body[0].bookingid);
}
